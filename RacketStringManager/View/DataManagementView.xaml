<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RacketStringManager.View.DataManagementView"
             Title="DataManagementView">
    <VerticalStackLayout>
        <Label 
            Text="Welcome to .NET MAUI!"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />

        <Button Margin="5" Command="{Binding ExportDatabaseCommand}" Text="Export to Excel"></Button>
        <Button Margin="5" Command="{Binding ImportDatabaseCommand}" Text="Import from Excel"></Button>
    </VerticalStackLayout>
</ContentPage>