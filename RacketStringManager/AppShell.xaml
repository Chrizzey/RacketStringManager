<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="RacketStringManager.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:RacketStringManager"
    xmlns:views="clr-namespace:RacketStringManager.View">

    <FlyoutItem Title="Overview">
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate local:MainPage}"></ShellContent>
        </Tab>
    </FlyoutItem>

    <FlyoutItem Title="New Job">
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate views:CreateJobPage}"></ShellContent>
        </Tab>
    </FlyoutItem>

    <FlyoutItem Title="Data Management">
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate views:DataManagementView}"></ShellContent>
        </Tab>
    </FlyoutItem>

    <!--<ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />-->

</Shell>
